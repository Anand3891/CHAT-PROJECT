		       'OPR':{'fullName':'Opera','Lstvertion':'22+',"upgradelink":"http://www.opera.com/computer/windows"} //no i18n
		       };
		PortalUI.unSupporedBrowserUI(browsersname);
	
				// Enable this script when mix panel used in client side
				//PortalUI.addMixPanelCode("f6bf3d79ce7aafb0eaf76357d69b6991");
	
	ListViewUI.setViewIds(UserPref.userConfig.viewcolumns.indexes);
	ListViewUI.init();
		if(!true)
		{
			CommonUtil.showannouncement = false;// No need to show the announcement , Tips and Tricks for the embed script unused portals
		}
		var soid = CurrentUser.getWMSID().split('_')[1];
		PortalUI.showEmbedUnusedAlert = !(true || Cookie.get(soid+"_embednotifyreminder") == "true" || false);
		PortalUI.checkAndNotifyToEmbedCode();
		UserUI.handleUpdateStatus();
		DN.checkDeskNotiPermission();
		
		
		if(soid == "21469597" || soid == "21535453")
		{
			if (window.addEventListener)
			{
				window.addEventListener("online", PortalUI.checkWMSAfterNetworkUp);
			} 
			else 
			{
				window.attachEvent("online",PortalUI.checkWMSAfterNetworkUp);
			} 	
		}
		if(typeof CrmPlusImpl !== "undefined")
		{	
			CrmPlusImpl.trigger("UIReady"); //NO I18N
		}
		var tranferInvites = CommonUtil.getFromLocalStorage("transferchat_invite_"+CurrentUser.getLSUID());//NO I18N
		//Showing transferinvite popups
		if(tranferInvites){
			tranferInvites = JSON.parse(tranferInvites);
			for (var key in tranferInvites) {
			  LSMessanger.transferChatInvite(tranferInvites[key]);
			}
		}
		
		Seasonal.init();
        PortalUI.handleTrackingLimit();
        //  avimpl.getUsersCallStatus();
	}
	catch(e){}
}

window.onbeforeunload=function()
{
        return PortalUI.handleOnbeforeUnload();
}

