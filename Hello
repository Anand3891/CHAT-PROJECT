function loadAudioInitIframe() {
	/*
		work around to overcome chrome's autoplay policy.
	*/
	if (! (/Chrome/.test(navigator.userAgent)) ) {
		document.getElementById("audioInitIframe").remove();
		return;
	}
	var extn = "mp3"; // No I18N
	if (new Audio().canPlayType("audio/wav") != "" && new Audio().canPlayType("audio/wav") != "no") {
		extn = "wav" // No I18N
	}
	document.getElementById("audioInitIframe").src = _SCHEMA + "://" + _SERVERURL+ '/sound/silence.' + extn;
}
loadAudioInitIframe();
</script>
