        ZDLibrary.Launcher.init({
        	icon : {
        		element : document.getElementById("onezoholauncher"),
                is_product_icon_available : false,
                is_hamburger_available: false,
                theme : 'dark' //No I18N
        	},
            menuBar: document.getElementById("productheader"),
            csrfName : _CFPARAMNAME,
            getCsrfValue : function(){
            	return Cookie.get(_CFCNAME);
            }
        })
};
